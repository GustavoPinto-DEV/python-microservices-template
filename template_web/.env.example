# Environment
ENVIRONMENT=dev

# Database
DB_USER=your_user
DB_PASSWORD=your_password
DB_HOST=localhost
DB_PORT=5432
DB_NAME=your_db

# JWT
SECRET_KEY=your-secret-key
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=120

# Web Server
WEB_HOST=0.0.0.0
WEB_PORT=8000

# Session
SESSION_SECRET_KEY=your-session-secret

# Logging
LOG_DIR_DEV=logs
LOG_DIR_EXTERNAL=/var/log/app/logs

# External APIs (si aplica)
EXTERNAL_API_URL=https://api.external.com
